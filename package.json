{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "A template for react and typescript.",
  "main": "./src/main.ts",
  "scripts": {
    "dev": "npm run serve",
    "serve": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:zip": "cross-env BUILD_ZIP=true npm run build",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress",
    "lint": "eslint ./src --ext .ts,.js",
    "lint:fix": "eslint ./src --ext .ts,.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likuner/react-ts-template.git"
  },
  "keywords": [
    "react",
    "typescript",
    "template",
    "webpack"
  ],
  "author": "Hunter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/likuner/react-ts-template/issues"
  },
  "homepage": "https://github.com/likuner/react-ts-template#readme",
  "dependencies": {
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
